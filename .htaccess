RewriteEngine on

RewriteRule ^(index|contact|registration|search|login)$ $1.php [NC,L]

RewriteRule ^post/(\d+)$ post.php?p_id=$1 [NC,L]

RewriteRule ^category/(\d+)$ category.php?c_id=$1 [NC,L]

RewriteRule ^index/(\d+)$ index.php?page=$1 [NC,L]

RewriteRule ^author/(\w+\s+\w+)$ author.php?author=$1 [NC,L]

RewriteRule ^author/(\w+)$ author.php?author=$1 [NC,L]

RewriteRule ^author/uploaded/(\w+)$ author.php?uploaded_by=$1 [NC,L]